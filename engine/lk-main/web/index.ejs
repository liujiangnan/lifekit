<link rel="stylesheet" href="lk-main/web/lib/element-ui/lib/theme-chalk/index.css">
<link rel="stylesheet" href="lk-main/web/index.css">

<el-container id="app" style="height:100%;">
  <el-header style="height:61px;">
    <el-row :gutter="20">
      <el-col :span="3">
        <img src="lk-main/web/lib/images/logo.png" style="height:60px;" alt="">
      </el-col>
      <el-col :span="18">
        <el-menu :default-active="activeIndex" class="el-menu-demo" mode="horizontal" @select="handleSelect">
          <el-menu-item index="1">处理中心</el-menu-item>
          <el-submenu index="2">
            <template slot="title">我的工作台</template>
            <el-menu-item index="2-1">选项1</el-menu-item>
            <el-menu-item index="2-2">选项2</el-menu-item>
            <el-menu-item index="2-3">选项3</el-menu-item>
          </el-submenu>
          <el-menu-item index="3">订单管理</el-menu-item>
        </el-menu>
      </el-col> 
      <el-col :span="3">
        <el-dropdown>
          <span class="el-dropdown-link">
            您好！admin<i class="el-icon-arrow-down el-icon--right"></i>
          </span>
          <el-dropdown-menu slot="dropdown">
            <el-dropdown-item><i class="el-icon-edit-outline"></i>修改密码</el-dropdown-item>
            <el-dropdown-item><i class="el-icon-upload2"></i>退出</el-dropdown-item>
          </el-dropdown-menu>
        </el-dropdown>
      </el-col>
    </el-row> 
    <div class="line"></div> 
  </el-header>
  <el-container>
    <el-aside width="200px">
      <el-row>
        <el-col :span="24">  
          <div @click="changeCollapse" class="menuhead">
              <i class="el-icon-menu"></i>
          </div>
          <el-menu default-active="2" class="el-menu-vertical-demo" avcdd="false">
            <el-menu-item index="1">
              <i class="el-icon-location"></i>
              <span  slot="title">导航一</span>
            </el-menu-item>
            <el-menu-item index="2">
              <i class="el-icon-menu"></i>
              <span slot="title">导航二</span>
            </el-menu-item>
            <el-menu-item index="3">
              <i class="el-icon-setting"></i>
              <span slot="title">导航三</span>
            </el-menu-item>
          </el-menu>
        </el-col> 
      </el-row>
    </el-aside>
    <el-main>Main</el-main>
  </el-container>
</el-container>
<script type="text/javascript" src="lk-main/web/index.js"></script>